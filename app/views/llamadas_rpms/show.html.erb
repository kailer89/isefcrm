<%- model_class = LlamadasRpm -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:realizadas) %>:</strong></dt>
  <dd><%= @llamadas_rpm.realizadas %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pendientes) %>:</strong></dt>
  <dd><%= @llamadas_rpm.pendientes %></dd>
  <dt><strong><%= model_class.human_attribute_name(:por_realizar) %>:</strong></dt>
  <dd><%= @llamadas_rpm.por_realizar %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              llamadas_rpms_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_llamadas_rpm_path(@llamadas_rpm), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              llamadas_rpm_path(@llamadas_rpm),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
