<%nothaserror=true%>
<%@errordetails = @errordetails%>
<h1>Imports</h1>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Prospecto Fallido</th>
      <th>Error</th>
    </tr>
  </thead>
  <tbody>
    <% @errordetails.each do |error| %>
    <%error.inspect%>
    <%nothaserror=false%>
      <tr>
        <td><%= error[0] %></td>
        <td><%= error[1] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%if nothaserror %>
<script type="text/javascript">
    window.location.href="/prospectos?validado=false"  // put your correct path in a string here
</script>
<%else%>
<script type="text/javascript">
    alert("Ocurrio un error con uno de los registros!");
</script>
<%end%>