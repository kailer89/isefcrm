                <th>ID</th>
                <th>Procendencia</th>
                <th>Nombre tarea</th>
                <th>Nombre prospecto</th>
                <th>Creado en </th>
                <th>Pendiente </th>
                <th>Actions</th>
              </tr>
            </thead>
            <tbody>
              <%if tareas != nil%>

                  <% tareas.each do |tarea| %>          
                    <tr class="<%if tarea.pendiente%>error<%end%>">
                      <td><%= link_to tarea.id, tarea_path(tarea) %></td>
                      <td><%= link_to "#{tarea.model_name}" , "/#{tarea.model_name}/#{tarea.model_id}/edit" %></td>
                      <%
nombreAUsar = ""                      
case tarea.model_name # a_variable is the variable we want to compare
when "prospectos"    #compare to 1
  nombreAUsar = Prospecto.where{id == tarea.model_id}.select([:id, :nombre,:apellido_paterno,:apellido_materno]).map {|e| {id: e.id, nombre: e.nombre, apellido_paterno: e.apellido_paterno, apellido_materno: e.apellido_materno} } rescue "Sin Valor"
when "solicitantes"     #compare to 2
  nombreAUsar = Solicitante.where{id == tarea.model_id}.first.prospectos.select([:id, :nombre,:apellido_paterno,:apellido_materno]).map {|e| {id: e.id, nombre: e.nombre, apellido_paterno: e.apellido_paterno, apellido_materno: e.apellido_materno} } rescue "Sin Valor"
when "examinados"
  nombreAUsar = Examinado.where{id == tarea.model_id}.first.solicitantes.first.prospectos.select([:id, :nombre,:apellido_paterno,:apellido_materno]).map {|e| {id: e.id, nombre: e.nombre, apellido_paterno: e.apellido_paterno, apellido_materno: e.apellido_materno} }  rescue "Sin Valor" 
when "admitidos"
  nombreAUsar = Admitido.where{id == tarea.model_id}.first.examinados.first.solicitantes.first.prospectos.select([:id, :nombre,:apellido_paterno,:apellido_materno]).map {|e| {id: e.id, nombre: e.nombre, apellido_paterno: e.apellido_paterno, apellido_materno: e.apellido_materno} } rescue "Sin Valor"    
when "inscritos"
  nombreAUsar = Inscrito.where{id == tarea.model_id}.first.admitidos.first.examinados.first.solicitantes.first.prospectos.select([:id, :nombre,:apellido_paterno,:apellido_materno]).map {|e| {id: e.id, nombre: e.nombre, apellido_paterno: e.apellido_paterno, apellido_materno: e.apellido_materno} }  rescue "Sin Valor"
